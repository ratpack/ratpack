<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%-4relative [%thread] %-5level %logger{35} - %msg %n</pattern>
    </encoder>
  </appender>

  <!--
  <root level="INFO">
      <appender-ref ref="STDOUT"/>
  </root>
  -->

  <logger name="org.thymeleaf" level="info">
    <appender-ref ref="STDOUT"/>
  </logger>

</configuration>